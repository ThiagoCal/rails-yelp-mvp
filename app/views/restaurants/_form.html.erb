  <%= simple_form_for  @restaurant do |f| %>
    <%= f.input :name %>
    <%= f.label :category, style: "display:block"%>
    <%= f.select :category, collection: Restaurant::CATEGORY %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.submit "Create new restaurant", class: "btn btn-primary" %>
  <% end %>
